@{
    ViewBag.Title = "无权限";
    Layout = "~/Areas/Home/Views/Shared/_Layout.cshtml";
}
@Html.StaticFor("/css/home/backstage.css")
@Html.StaticFor("/css/home/common.css")
<div class="m-opersf f-cb">
    <dl>
        <dt class="ico-fail"></dt>
        <dd>
            <h1 class="z-err">
                对不起,您无权限访问此页面</h1>
            <p>
                将在<em class="z-err">@(int.Parse(ViewData["time"].ToString()))</em>秒后自动跳转，若不想等待，请<a
                    href="javascript:history.go(-1);">点击此处</a>跳转</p>
            <p>
                <i class="z-err"></i>
            </p>
        </dd>
    </dl>
</div>
<script type="text/javascript">
    setTimeout(function () {
        history.go(-1);
    }, parseInt('@ViewData["time"].ToString()') * 1000);
</script>
